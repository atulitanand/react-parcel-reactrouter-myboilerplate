{
  "name": "reactrouter-parcel",
  "version": "2.0.1",
  "main": "index.js",
  "license": "MIT",
  "keywords": [
    "react",
    "parcel",
    "bootstrap"
  ],
  "description": "my light weight react boilerplate with parcel support",
  "scripts": {
    "start": "parcel ./src/index.html --open",
    "format": "npx prettier --write src/.",
    "lint": "eslint \"src/**/*.{js, jsx}\" --quiet",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html"
  },
  "devDependencies": {
    "eslint": "^7.29.0",
    "eslint-plugin-react": "^7.24.0",
    "parcel": "^2.0.0-beta.3.1",
    "prettier": "2.3.2",
    "@babel/cli": "*",
    "@babel/core": "*",
    "@babel/eslint-parser": "*",
    "@babel/plugin-proposal-class-properties": "*",
    "@babel/preset-env": "*",
    "@babel/preset-react": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-react-hooks": "*"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/icecoffee-dev/react-parcel-reactrouter-myboilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/icecoffee-dev/react-parcel-reactrouter-myboilerplate/issues"
  },
  "homepage": "https://github.com/icecoffee-dev/react-parcel-reactrouter-myboilerplate/#readme"
}
